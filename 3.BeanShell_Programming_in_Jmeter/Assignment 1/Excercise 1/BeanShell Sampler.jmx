        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="BeanShell Call tiki.vn" enabled="true">
          <stringProp name="BeanShellSampler.query">import java.net.HttpURLConnection;
import java.net.URL;
import java.nio.charset.StandardCharsets;
import org.apache.commons.io.IOUtils;

URL url = new URL(&quot;https://api.tiki.vn/personalization/v2/products?page=1&amp;limit=10&quot;);
HttpURLConnection con = (HttpURLConnection) url.openConnection();
con.setRequestMethod(&quot;GET&quot;);
String response = IOUtils.toString(con.getInputStream(), StandardCharsets.UTF_8);
return response;</stringProp>
